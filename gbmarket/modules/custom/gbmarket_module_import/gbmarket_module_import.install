<?php

/**
 * Implements hook_install().
 */
function gbmarket_module_import_install() {
  // Set up vocabulary for imported modules categories.
  $vocabulary = (object) array(
    'name' => 'Module categories',
    'machine_name' => 'module_categories',
    'description' => 'Categories (types) for imported modules.',
  );
  taxonomy_vocabulary_save($vocabulary);

  cache_clear_all();
}
