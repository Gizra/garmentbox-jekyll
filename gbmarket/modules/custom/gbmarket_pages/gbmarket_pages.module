<?php
/**
 * @file
 * Code for the gbmarket pages feature.
 */

include_once 'gbmarket_pages.features.inc';


/**
 * Implements hook_ctools_plugin_directory().
 */
function gbmarket_pages_ctools_plugin_directory($module, $plugin) {
  if ($module == 'ctools' && !empty($plugin)) {
    return "plugins/$plugin";
  }
}
